using System;
public class Indexer
{
    string[] values=new string[10];
    public Indexer()
    {
        for(int i=0;i<values.Length;i++)
        values[i]="empty";
        
    }

    public string this[int index]
    {
        get
        {
            if(index>=0 && index<values.Length)
            return values[index];
            else
            throw new IndexOutOfRangeException("Invalid");
        }
